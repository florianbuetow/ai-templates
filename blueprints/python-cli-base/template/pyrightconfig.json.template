{
  "pythonVersion": "{{python_version}}",
  "pythonPlatform": "Darwin",
  "venvPath": ".",
  "venv": ".venv",
  "stubPath": "stubs",
  "include": [
    "src",
    "tests"
  ],
  "exclude": [
    "**/__pycache__",
    "**/.pytest_cache",
    "**/node_modules",
    "**/.venv"
  ],
  "typeCheckingMode": "strict",
  "useLibraryCodeForTypes": true,
  "reportMissingImports": true,
  "reportMissingTypeStubs": true
}
